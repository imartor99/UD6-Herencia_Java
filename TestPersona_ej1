public class Persona{
    String nif;
    int altura;
    int edad;
    
    //Constructor por defecto
    public Persona (){
        nif = "11111111A";
        altura = 175;
        edad = 25;
    }

    //Constructor dandole valores 
    public Persona (String nif, int altura, int edad){
        this.nif=nif;
        this.altura = altura;
        this.edad = edad;
    }

    //Getters y Setters
    public String getNif(){
        return nif;
    }

    public int getAltura(){
        return altura;
    }

    public int getEdad(){
        return edad;
    }

    public void setNif(String nif){
        this.nif=nif;
    }

    public void setAltura(int altura){
        this.altura=altura;
    }

    public void setEdad(int edad){
        this.edad=edad;
    }
    
    @Override
    public String toString() {
        return "Persona: " + nif + altura + " cm" + edad + " a√±os";
    }
}

public class TestPersona {
    public static void main(String[] args) {
        //Creo persona por defecto
        Persona p1 = new Persona();
        //Creo la persona dandole los datos
        Persona p2 = new Persona("11223344C", 180, 25);

        //Imprimimos por pantalla los objetos persona
        System.out.println("p1");
        System.out.println("p2");
    }
}
